{
	"Version": "2012-10-17",
	"Statement": [{
			"Effect": "Allow",
			"Action": "iam:PassRole",
			"Resource": [
				"arn:aws:iam::*:role/dms-vpc-role",
				"arn:aws:iam::*:role/DataClassificationPipelineGlueJobRole",
				"arn:aws:iam::*:role/DataClassificationPipelineStartGlueWorkflowLambdaRole",
				"arn:aws:iam::*:role/DataClassificationPipelineCreateGlueScriptLambdaRole",
				"arn:aws:iam::*:role/DataClassificationPipelineKinesisRole",
				"arn:aws:iam::*:role/DataClassificationPipelineCloudWatchRole"
			]
		},
		{
			"Effect": "Allow",
			"Resource": "*",
			"Action": [
				"cloudformation:*Stack*",
				"cloudformation:*ChangeSet*",
				"cloudformation:*Template*",
				"dms:*Endpoint*",
				"dms:*ReplicationInstance*",
				"dms:*ReplicationSubnetGroup*",
				"dms:*ReplicationTask*",
				"dms:*Certificate*",
				"dms:*TableStatistics*",
				"dynamodb:*Table*",
				"dynamodb:*Limits*",
				"ec2:*Address*",
				"ec2:*Instance*",
				"ec2:*InternetGateway*",
				"ec2:*Route*",
				"ec2:*SecurityGroup*",
				"ec2:*Subnet*",
				"ec2:*Tags*",
				"ec2:*Volume*",
				"ec2:*Vpc*",
				"ec2:*AccountAttributes*",
				"events:*Rule*",
				"events:*Targets*",
				"firehose:*DeliveryStream*",
				"glue:*Crawler*",
				"glue:*Database*",
				"glue:*Job*",
				"glue:*Trigger*",
				"glue:*Table*",
				"glue:*Workflow*",
				"glue:GetCatalogImportStatus",
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:GetRole",
				"iam:ListRoles",
				"iam:*Policy",
				"iam:*Policies",
				"kms:*Alias*",
				"kms:*Grant*",
				"kms:*Key*",
				"kms:Decrypt*",
				"kms:Encrypt*",
				"kms:*ResourceTags*",
				"kms:ReEncryptFrom*",
				"kms:ReEncryptTo*",
				"kms:TagResource*",
				"kms:UntagResource*",
				"lambda:*Permission*",
				"lambda:*Function*",
				"lambda:*Tags*",
				"rds:*DBInstance*",
				"rds:*DBSubnetGroup*",
				"rds:*DBSecurityGroup*",
				"logs:Describe*",
				"logs:Get*",
				"logs:List*",
				"logs:StartQuery",
				"logs:StopQuery",
				"logs:TestMetricFilter",
				"logs:FilterLogEvents",
				"s3:*Bucket*",
				"s3:*Object*",
				"s3:*EncryptionConfiguration*",
				"tag:*",
				"macie2:*",
				"athena:*"
			]
		}
	]
}